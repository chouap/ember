<form role="form">
    <div class="form-group">
        <label for="nom">Nom</label>
        {{input class="form-control" disabled="disabled" type="text" name="nom" value=model.nom}}
    </div>
    <div class="form-group">
        <label for="type">Type</label>
        {{input class="form-control" disabled="disabled" type="text" name="type" value=model.type}}
